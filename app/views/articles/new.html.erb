<%= notice %>
<%= form_for @article do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :content %>
    <%= f.text_area :content %>
  </div>
  <div>
    <%= f.label :published %>
    <%= f.check_box :published %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>