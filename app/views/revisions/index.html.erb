<h1>Revisions</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>Title</th>
      <th>Article</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @revisions.each do |revision| %>
      <tr>
        <td><%= revision.content %></td>
        <td><%= revision.title %></td>
        <td><%= revision.article_id %></td>
        <td><%= revision.user_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

